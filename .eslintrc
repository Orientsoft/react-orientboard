{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "defaults"
  ],
  "rules": {
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "never"],
    "comma-style": [2, "first"],
    "no-unused-vars": 2,
    "no-console": 0,
    "no-undef": 2,
    "react/jsx-uses-react": 1,
    "max-len": [2, 80, 2]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  }
}
